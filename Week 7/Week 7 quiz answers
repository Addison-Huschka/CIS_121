#1
def is_isogram(word):
    seen_letters = {}
    for letter in word:
        if letter in seen_letters:
            return False           
        seen_letters[letter] = True
    return True

#2
def find_unique(numbers):
    counts = {}
    for num in numbers:
        if num in counts:
            counts[num] += 1
        else:
            counts[num] = 1
    for num, count in counts.items():
        if count == 1:
            return num

#3
def return_unique(numbers):
    frequency = {}
    for num in numbers:
        frequency[num] = frequency.get(num, 0) + 1
    unique_numbers = []
    for num, count in frequency.items():
        if count == 1:
            unique_numbers.append(num)
    return unique_numbers

#4
def get_names(names):
    return list(names.values())

#5
def find_oldest(people):
    oldest_person = max(people, key=people.get)
    return oldest_person

#6
def letter_count(word):
    count = {}
    for letter in word:
        if letter in count:
            count[letter] += 1
        else:
            count[letter] = 1
    return count

#7
def min_grade(exams):
    lowest_grade = min(exams, key=exams.get)
    return lowest_grade

#8
def find_youngest(people):
    youngest_person = min(people, key=people.get)
    return youngest_person

#9
dictionary = {}
dictionary["Side Salad"] = 6
dictionary["Chicken Parm"] = 12
dictionary["Cookie"] = 3
def add_contents(order):
    total = 0
    for items in order:
        total += order[items]
    return total
print(dictionary)
print(add_contents(dictionary))

#13
def total_sales(sales):
    total = 0
    for sold in sales.values():
        total += sold
    return total