print("start")

try:
    print("enter a number. I will divide 10 by that number and output the result.")
    user_input = input("pick a number: ")
    user_number = int(user_input)
    result = 10 / user_number
    print(f'result = {result}')
except ValueError:
    print("That is not a valid number. Please enter an integer.")
except ZeroDivisionError:
    print("You cannot divide by zero. Please enter a non-zero integer.")

print("end")