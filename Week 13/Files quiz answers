#7
with open("LibraryVisits.csv", "r") as file:
    total_days = 0
    total_visits = 0
    for each_line in file:
        items = each_line.split(",")
        total_days += 1
        total_visits += int(items[1])
print(f'Average Visits: {total_visits / total_days}')