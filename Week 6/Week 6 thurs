my_word = "Peter Piper picked a peck of pickled peppers"

def string_to_list_vowels(word):
    words = []
    built_word = ""
    vowel_count = 0
    for letter in word:
        print(letter, vowel_count, built_word)
        if letter == " ":
            if vowel_count >= 2:
                words.append(built_word)
                #vowel_count = 0
            built_word = ""
        else:
            built_word += letter
            if letter in "aeiou":
                vowel_count += 1
    words.append(built_word)
    return words
print(string_to_list_vowels(my_word))