import math
class Planet:
    def __init__(self, name, radius, mass, distance):
        self.name = name
        self.radius = radius
        self.mass = mass
        self.distance = distance
    def get_name(self):
        return self.name
    def get_radius(self):
        return self.radius
    def get_mass(self):
        return self.mass
    def get_distance(self):
        return self.distance
    def get_volume(self):
        volume = (4/3) * (math.pi) * (self.radius**3)
        return volume

    