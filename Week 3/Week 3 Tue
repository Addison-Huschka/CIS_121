user_num1 = float(input("Give me a number: "))
user_num2 = float(input("Give me another number: "))
operator = input("pick: + - * / ")
if operator == "+":
    print(f'Here is the sum of your numbers: {user_num1+user_num2}')
elif operator == "-":
    print(f'Here is the difference of your numbers: {user_num1-user_num2}')
elif operator == "*":
    print(f'Here is the product of your numbers: {user_num1*user_num2}')
elif operator == "/":
    print(f'Here is the dividend of your numbers: {user_num1/user_num2}')
else:
    print("error")