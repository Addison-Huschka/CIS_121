class BankAccount: 
    def __init__(self, name, initial_balance = 0): 
        self.owner = name 
        self.balance = initial_balance 
    def deposit(self, value): 
        self.balance += value 
    def withdraw(self, value): 
        if value > self.balance: 
            print('insufficent funds') 
        else: 
            print(f'here is your money: ${value}') 
            self.balance -= value
    def get_owner(self): 
        return self.owner 
    def get_balance(self): 
        return self.balance 
    def set_owner(self, new_owner): 
        self.owner = new_owner 
    def __str__(self): 
        msg = '' 
        msg += f'owner: {self.owner}\nbalance: {self.balance}' 
        return msg 
    def __add__(self,other):
        new_owner = f'{self.get_owner()} & {other.get_owner()}'
        new_balence = self.get_balance() + other.get_balance()
        print(new_balence)

matt_acc = BankAccount('Matt') 
matt_acc.deposit(100) 
matt_acc.deposit(50) 

ashley_acc = BankAccount("Ashley",500)

matt_acc.__add__(ashley_acc)